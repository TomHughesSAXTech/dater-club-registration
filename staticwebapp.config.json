{
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin.html"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.css", "*.js", "*.png", "*.jpg", "*.svg", "*.ico"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
